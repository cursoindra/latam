package diauno.primerejercicio.hilos;

public class VerHilo
{

	public static void main(String[] args)
	{
		Hilo hilo= new Hilo(5000);
		hilo.start();
		
		Hilo hilo1= new Hilo(10000);
		hilo1.start();
		
		Hilo hilo2= new Hilo(20000);
		hilo2.start();
		
		Hilo hilo3= new Hilo(30000);
		hilo3.start();
		
		Hilo hilo4= new Hilo(40000);
		hilo4.start();
	}

}
